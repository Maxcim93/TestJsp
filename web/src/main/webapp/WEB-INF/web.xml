<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ScheduleViewServlet</servlet-name>
    <servlet-class>com.maxim.web.ScheduleViewServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddDoctorServlet</servlet-name>
    <servlet-class>com.maxim.web.AddDoctorServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddPatientServlet</servlet-name>
    <servlet-class>com.maxim.web.AddPatientServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ScheduleViewServlet</servlet-name>
    <url-pattern>/clinic/schedule</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddDoctorServlet</servlet-name>
    <url-pattern>/clinic/doctor</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddPatientServlet</servlet-name>
    <url-pattern>/clinic/patient</url-pattern>
  </servlet-mapping>

</web-app>